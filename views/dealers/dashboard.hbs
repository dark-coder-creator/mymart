                  <th>Priority</th>
                </tr>
              </tfoot>
              <tbody>
                 {{#each products}}
                <tr>
                 
               
                  <tr>
             
              <td>{{this.Name}}</td>
            <td>{{this.Category}}</td>
            <td>â‚¹{{this.Price}}</td>
         
           <td><img style="width:50px;height:50px" src="/product-images/{{this._id}}.jpg" alt=""></td>
       <td>
        <a href="/dealers/edit-product/{{this._id}}" class="btn btn-primary" onclick="return confirm('Are you want to edit {{this.Name}}?')">Edit</a>
            <a href="/dealers/delete-product/{{this._id}}" class="btn btn-danger"
              onclick="return confirm('Are you want to delete {{this.Name}}?')">Delete</a>
      </td>
      
    </tr>
                 {{/each}}
              </tbody>
            </table>
          </div>
        </div>
